<div>
	<section id="nw-profile">
		<h2 class="hidden">Profile</h2>
		<div id="logo-img">
			<img id="profile-image" class="img-circle" src="http://gazettereview.com/wp-content/uploads/2016/03/facebook-avatar.jpg" alt="Profile image"><br>
		</div>
		<div id="profile-context">
			<p>Hi! I'm Namwon</p>
			<p> Curious, Logical, Intuitive</p>
			<%= render :template => "specs/index" %>
		</div>
	</section><br>
	<hr>
	<section id="nw-language">
		<h2 class="mainview-title">Language skills</h2><br>
		<div id="language-context" class="container">
			<img class="flag" src="https://lipis.github.io/flag-icon-css/flags/4x3/kr.svg" alt="South Korea Flag">
			<img class="flag" src="https://lipis.github.io/flag-icon-css/flags/4x3/um.svg" alt="United States Minor Outlying Islands Flag">
			<img class="flag" src="https://lipis.github.io/flag-icon-css/flags/4x3/ru.svg" alt="Russia Flag">
			<img class="flag" src="https://lipis.github.io/flag-icon-css/flags/4x3/cn.svg" alt="China Flag">
		</div>
	</section><br>
	<hr>
	<section id="nw-design">
		<h2 class="mainview-title">Design</h2>
		<div>
			<%= render :template => "designs/index" %>
		</div>
	</section><br>
	<hr>
	<section id="nw-development">

			<h2 class="mainview-title">Development</h2><br>
			<h4 id="work-on">Working On It</h4>

	</section><br>
</div>

<hr>

<footer>
	<button class="sns-btn btn-default"><a href="https://www.linkedin.com/in/namwon-heo-20ba2679">LinkedIn</a></button>
	<!--유저 로그인 -->
	<div class="login">
		<% if user_signed_in? %>
		  <p>현재 로그인된 유저: <%= current_user.email %><span>
		  <%= link_to "로그아웃", destroy_user_session_path, method: :delete, data: { confirm: "Are you sure?" } %></p>
		<% else %>
		  <p><%= link_to "로그인", new_user_session_path %></p>
		<% end %>
	</div>
	<!-- 로그인 끝 -->
</footer>
