<div class="container">


	<div>
		<h2 name="nw-profile">Profile</h2>
			<%= render :template => "specs/index" %>
		
		<section id="nw-design">
			<h2 name="nw-design">Design</h2>
			<%= render :template => "designs/index" %>
		</section>
	</div>

</div>

<footer>
	<!--유저 로그인 -->
	<% if user_signed_in? %>
	  <p>현재 로그인된 유저의 이메일 : <%= current_user.email %></p>
	  <p><%= link_to "로그아웃", destroy_user_session_path, method: :delete, data: { confirm: "Are you sure?" } %></p>
	<% else %>
	  <p><%= link_to "로그인", new_user_session_path %></p>
	<% end %>
	<!-- 로그인 끝 -->
</footer>
